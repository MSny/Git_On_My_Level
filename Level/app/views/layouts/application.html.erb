<!DOCTYPE html>
<html>
<head>
 <title>Level</title>
  <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track' => true %>
  
  <%= csrf_meta_tags %>
  <%= render 'layouts/shim' %>
  <meta name=viewport content="width=device-width, initial-scale=1">
</head>
<body>
<%= render 'layouts/header' %>
<script>
var userParam = <%= @user_param %>;
console.log("This is userParam: "+userParam);
</script>

<% if @user && current_user %>
<script>
var signedInUserId = <%= session[:user_id] %>
console.log("This is signed in user "+signedInUserId)
</script>
<%end%>
<%= render '/shared/session_links' %>
<% flash.each do |message_type, message| %>
<div class="alert alert-<%= message_type %>"><%= message %>
</div>
<%end%>
<%= javascript_include_tag 'application' %>
<!-- Devicons for skill badges -->
  <link rel="stylesheet" href="https://cdn.rawgit.com/konpa/devicon/89f2f44ba07ea3fff7e561c2142813b278c2d6c6/devicon.min.css">

  <!-- Materialize -->
  <!-- Compiled and minified CSS -->
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.96.1/css/materialize.min.css">

  <!-- Compiled and minified JavaScript -->
  <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.96.1/js/materialize.min.js"></script>


  <!--Let browser know website is optimized for mobile-->
  <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no"/>
<%= yield %>

</body>
</html>
